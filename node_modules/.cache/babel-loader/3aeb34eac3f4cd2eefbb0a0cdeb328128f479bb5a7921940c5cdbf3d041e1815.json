 /*#__PURE__*/_jsxDEV(\"hr\", {}, void 0, false, {\n      fileName: _jsxFileName,\n      lineNumber: 35,\n      columnNumber: 7\n    }, this), /*#__PURE__*/_jsxDEV(EventCountdown, {\n      timer: 9\n    }, void 0, false, {\n      fileName: _jsxFileName,\n      lineNumber: 36,\n      columnNumber: 7\n    }, this)]\n  }, void 0, true, {\n    fileName: _jsxFileName,\n    lineNumber: 8,\n    columnNumber: 5\n  }, this);\n}\n_c = App;\nexport default App;\nvar _c;\n$RefreshReg$(_c, \"App\");","map":{"version":3,"names":["React","TextFolding","EventCountdown","TextEllipsis","App","width"],"sources":["/Users/chencheng/Library/Containers/com.tencent.xinWeChat/Data/Library/Application Support/com.tencent.xinWeChat/2.0b4.0.9/1dc64184dfec0ae764b796cdaee53fde/Message/MessageTemp/11e579a58271ad70da616d26244756d1/File/react-test/src/components/Other/index.js"],"sourcesContent":["import React from \"react\";\nimport { TextFolding } from \"../TextFolding\";\nimport { EventCountdown } from \"../EventCountdown\";\nimport TextEllipsis from \"../TextEllipsis\";\n\nfunction App() {\n  return (\n    <div className=\"App\">\n      <TextFolding\n        text=\"卡高消费卡背影删掉疯狂国际化设计的咖啡杯个双方都赶不上看对方还不够舒适的方式地方就会感慨阿妈打卡高消费卡背影够舒适的方方就会感慨阿妈打卡高消费卡背影删掉疯狂国际化设计的咖啡杯个双方都赶不上看对方还不够舒适的方式地方就会感慨阿妈打卡高消费卡背\"\n        maxRow={4}\n      />\n      <hr />\n      <div\n        style={{\n          width: 400,\n        }}\n      >\n        <TextEllipsis content=\"高消费卡背影删掉疯狂国际化设计的咖啡杯个双方都赶不上看对方还不够舒适的方式地方就会感慨阿妈打山西监管局：全面贯彻落实综合考核评价机制\n狠抓三项工作奠定考核基础\n　　完善综合考核评价利用反馈机制，是贯彻落实财政部党组工作要求，展现财政监管工作干事创业激情，推动财政监管事业高质量转型发展的必然要求。考核的目的在于运用，全面贯彻落实综合考核评价机制，应从抓思想基础、抓业务基础、抓转型基础三方面协同推进，切实发挥综合考核“指挥棒”作用，全面推动全局中心工作不断迈上新台阶。\n\n　　一、抓思想基础，提升工作主动性 \n\n　　一是加强干部思想工作考核力度。干部是完成工作任务的重要基础，应通过严格干部考核管理，打造一支政治严明、组织观念和大局意识强的干部队伍。要不断提高干部对考核评价工作的认识，制定干部自我评价的办法，切实把单位考核与个人评价结合起来，充分调动干部想干事、能干事、干成事的意识，不断夯实考核评价基础。二是提升干部大局意识。考核评价应充分重视对干部大局观的培养，引导教育干部职工全面贯彻落实党中央重大决策部署和习近平总书记重要指示批示精神，推动重大财税政策落地实施，主动关注国际国内财经动态、财政监管最新变化和最前瞻性研究，对标对表各级监管机构和系统内优秀监管局，采取积极措施，不断提高干部职工干事创业的政治觉悟和思想自觉。教育指导干部弄懂专业知识，提高业务水平，熟练应用财经法律法规，不只局限于了解财政专业知识和国内外财经方面监管动态，还应熟知社会各类经济领域信息和基础知识，确保深刻领悟上级部门的政策意图，更好发挥财政监管职能作用，充分体现财政监管局“探头”“哨兵”“参谋”作用。三是提升干部学习意识。随着新技术的广泛应用，面对知识呈现更新时间短，传递速度快、多样多层次的新特点，应积极鼓励党员干部养成爱读善学的习惯，选择有益于财政监管工作的国内外经济著作、视频资料等开展学习，组织开展青年研究论坛等形式，促进干部知识储备和人才储备更新，保证财政监管工作持续保有活力、创新性和可持续性。 \n\n　　二、抓业务基础，把握工作关键点 \n\n　　一是强化监管重点和难点。我们既要善于判断重点监管工作，准确贯彻落实上级部门指示精神和工作要点，按照事件的突发性、重要性等因素进行先后排序，确定工作重点；还应结合过往监管经验，充分理解和把握财政政策，融合当地经济发展和财政收支实际情况，选取重点、难点工作开展监管。二是评价业务规范和风险控制。监管工作要在新时代有新作为，工作开展过程中应准确贯彻落实全国监管局工作会议精神，并坚持将质量和风险贯穿于始终。首先，要进一步加强与时代要求相适应的制度建设，进一步加大紧跟新征程步伐要求的制度建设，不断修订和完善相关制度。其次，要借助互联网信息技术，推动形成监管业务向线上监管转移，线下监管以调研为主要内容的监管新业态，同时充分利用大数据夯实研究分析应用基础，逐步解决监管局人少事多的矛盾。三是突出反映区域监管特色。坚持业务监管区域特色，实现“一区域一政策”，充分发挥考核机制引导推动作用。坚持把“人民至上”牢牢贯穿于监管工作全过程，客观评价不同区域监管工作中，促进发挥业务监管新突破、监管价值新提高、监管打开新局面的重要作用。 \n\n　　三、抓转型基础，提高工作实效性 \n\n　　一是培养专家型监管队伍。应坚持将专业的事交给专业的人去做，引导考核工作向岗位精细化、干部专业化、工作方法创新化方向倾斜。二是注重调查研究，当好“哨兵”“参谋”。监管局“哨兵”“参谋”作用的发挥，离不开深入实际的调查研究，应坚持在调研中摸清问题的根源，精准反映问题的成因，提供有价值的材料。只有在源头上铲除问题，才能有效降低监管成本，提高监管效益。因此，在考核中应把调查研究工作及成果应用放在突出位置。三是侧重问题整改纠正。偏重考核对整改结果的重视，要坚持将监管成果的应用重点放在监管发现问题的整改当中。要坚持“查、督、帮、纠、改”五位一体，形成彻底排除监管问题的闭环管理。 卡高消费卡背影够舒适的方方就会感慨阿妈打卡高消费卡背影删掉疯狂国际化设计的咖啡杯个双方都赶不上看对方还不够舒适的方式地方就会感慨阿妈打卡高消费卡背\" />\n      </div>\n      <hr />\n      <EventCountdown timer={9} />\n    </div>\n  );\n}\n\nexport default App;\n"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,WAAW,QAAQ,gBAAgB;AAC5C,SAASC,cAAc,QAAQ,mBAAmB;AAClD,OAAOC,YAAY,MAAM,iBAAiB;AAAC;AAE3C,SAASC,GAAG,GAAG;EACb,oBACE;IAAK,SAAS,EAAC,KAAK;IAAA,wBAClB,QAAC,WAAW;MACV,IAAI,EAAC,ktBAA0H;MAC/H,MAAM,EAAE;IAAE;MAAA;MAAA;MAAA;IAAA,QACV,eACF;MAAA;MAAA;MAAA;IAAA,QAAM,eACN;MACE,KAAK,EAAE;QACLC,KAAK,EAAE;MACT,CAAE;MAAA,uBAEF,QAAC,YAAY;QAAC,OAAO,EAAC;MAc6U;QAAA;QAAA;QAAA;MAAA;IAAG;MAAA;MAAA;MAAA;IAAA,QAClW,eACN;MAAA;MAAA;MAAA;IAAA,QAAM,eACN,QAAC,cAAc;MAAC,KAAK,EAAE;IAAE;MAAA;MAAA;MAAA;IAAA,QAAG;EAAA;IAAA;IAAA;IAAA;EAAA,QACxB;AAEV;AAAC,KAjCQD,GAAG;AAmCZ,eAAeA,GAAG;AAAC;AAAA"},"metadata":{},"sourceType":"module","externalDependencies":[]}